#!/usr/bin/make -f
# -*- makefile -*-

# Uncomment this to turn on verbose mode.
#export DH_VERBOSE=1

%:
	dh $@

override_dh_auto_configure:
	dh_auto_configure -- -DBUILD_STATIC_LIBS=FALSE -DBUILD_SHARED_LIBS=TRUE

.PHONY: override_dh_strip

override_dh_strip:
	dh_strip --dbg-package=kmsjsoncpp-dbg
